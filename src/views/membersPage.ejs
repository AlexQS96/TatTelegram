<!DOCTYPE html>
<html lang="es">
<%- include('partials/head.ejs') %>
<body>
  <div class="page_Container">
    <%- include('partials/header.ejs') %>
    <main>
        <section class="gridPage">

            <% memberData.forEach(element => { %>
            <article class="member"><a href="/members/<%- element.id %>"><img class="avatarCard" src="../images/profileCards/<%- element.image[0] %>" alt="<%- element.name %>"></a></article>
            <% }) %>
        </section>
    </main>
    <%- include('partials/footer.ejs') %> 
  </div>
</body>
</html>